#!/bin/bash
if "true" : '''\'
then
python${ROS_PYTHON_VERSION:-} "${BASH_SOURCE[0]}" $*
exit
fi
'''
# flake8: noqa
import rclpy

from flexbe_input.flexbe_input import FlexBEInput

def main():
  node = rclpy.create_node('flexbe_input')
  be_input = FlexBEInput(node)

  # Wait for ctrl-c to stop the application
  rclpy.spin()

if __name__ == '__main__':
    main()
